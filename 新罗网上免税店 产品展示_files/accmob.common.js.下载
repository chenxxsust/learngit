ACCMOB.common={currentPath:window.location.pathname,visiblePopupId:"",bindFormLinks:function(){$(".formLink").live("click",function(){$(this).parent("form").submit();return false})},bindShowAllItems:function(){$(".showAllItems").live("click",function(){$("#cartTotals").show();var a=$(".itemsList").attr("numberOfItems");var b=$(".itemsList").attr("liFilterClass");$(".viewLess").removeAttr("style");$(this).hide();$(".itemsList").find("li").show().end().append($(".viewLess").live("click",function(){$(".itemsList li").filter("."+b).filter("li:gt("+a+")").hide();$(this).attr("style","display:none");$(".showAllItems").show();$("#cartTotals").hide()}))})},allLinks:function(){$("a:not([href*='#'], [href^=''], [href^='javascript'])").live("click",function(){$.mobile.showPageLoadingMsg()})},allFormPosts:function(){$('form[method="post"]').live("submit",function(){$.mobile.showPageLoadingMsg()})},bindShowProcessingMessageToSubmitButton:function(){$(":submit.show_processing_message").each(function(){$(this).on("click",ACCMOB.common.showPageLoadingMsg)})},showPageLoadingMsg:function(){$.mobile.showPageLoadingMsg()},hidePageLoadingMsg:function(){$.mobile.hidePageLoadingMsg()},bindLangAndCurrencySelectors:function(){$("#lang-selector").live("change",function(){$("#lang-form").submit()});$("#currency-selector").live("change",function(){$("#currency-form").submit()})},bindCarouselLink:function(){var a=$('ul[id|="carousel_"] > li a');if(a.length>0){a.colorbox()}},preventDefault:function(a){if(!(typeof a==="undefined")&&!!a){a.preventDefault()}},bindToSortForm:function(b){var a=$(".sort_form select");a.live("change",function(g){var e=$(this).find(":selected").val();var d=$.mobile.path.parseUrl(window.location.href);if(!!d.search){var c=d.search.split("%3A");c[1]=e;var f=c.join("%3A")}else{var f="?q=%3A"+e}window.location.href=d.hrefNoSearch+f})},bindSkipToContentLink:function(){$("a.skiptocontent").live("click",function(a){$($(this).attr("href")).find("a").first().focus();return true})},initialize:function(){with(ACCMOB.common){}}};$(document).ready(function(){ACCMOB.common.initialize()});$(document).bind("mobileinit",function(){var a=$.mobile.path.parseUrl(window.location.href);if(a.hash!==""&&!!a.hash.match(/^./)){$.mobile.hashListeningEnabled=false}});$.ajaxPrefilter(function(b,d,c){if(b.type==="post"||b.type==="POST"){var a=(typeof b.data==="undefined")||(b.data===null);if(b.multipartAjax){if(b.data==null){return}else{b.data.append("CSRFToken",ACCMOB.config.CSRFToken)}}else{if(a||b.data.indexOf("CSRFToken")===-1){b.data=(!a?b.data+"&":"")+"CSRFToken="+ACCMOB.config.CSRFToken}}}});