function getCookie(c){var b=document.cookie;var e=c+"=";var d=b.indexOf("; "+e);if(d==-1){d=b.indexOf(e);if(d!=0){return null}}else{d+=2}var a=document.cookie.indexOf(";",d);if(a==-1){a=b.length}return unescape(b.substring(d+e.length,a))}function setCookie(a,d,b){var e=new Date();e.setDate(e.getDate()+b);var c=escape(d)+((b==null)?"":"; path=/;expires="+e.toUTCString());document.cookie=a+"="+c}var session={ua:function(){var a=navigator.userAgent||navigator.vendor||window.opera;var b="";if(a.match(/iPad/i)||a.match(/iPhone/i)||a.match(/iPod/i)){b="iOS"}return b},isQuotaExceededError:function(){if(typeof sessionStorage==="object"){try{sessionStorage.setItem("key","value");sessionStorage.removeItem("key");return true}catch(a){return false}}else{return false}},set:function(a,b){if(this.isQuotaExceededError()){sessionStorage.setItem(a,b)}},del:function(a){if(this.isQuotaExceededError()){sessionStorage.removeItem(a)}},get:function(a){if(this.isQuotaExceededError()){return sessionStorage.getItem(a)}},useBack:function(){location.hash="b=true"},isBack:function(){var c=false;if(location.hash!=""){var a=location.hash.replace("#","").split("=");var b={};b[a[0]]=a[1];c=b.b}return c},resetBack:function(){setTimeout(function(){history.back()},100)}};var cookieStorage={set:function(a,b){$.cookie(a,b,{path:"/"})},del:function(a){$.removeCookie(a,{path:"/"})},get:function(a){return $.cookie(a)},useBack:function(){location.hash="b=true"},isBack:function(){var c=false;if(location.hash!=""){var a=location.hash.replace("#","").split("=");var b={};b[a[0]]=a[1];c=b.b}return c}};