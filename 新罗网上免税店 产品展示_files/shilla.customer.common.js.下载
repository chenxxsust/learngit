function submitCustomerFormAjax(a,g,d,f){if(a==null){return false}var h=typeof(d);if(h!="undefined"){if(h=="string"){var i=d}if(h=="number"){var i=d}if(h=="function"){var b=d}if(h=="object"){var c=d}}enableDim();var e=a.attr("action");a.removeAttr("action");$.ajax({type:"POST",url:e,cache:false,data:a.serialize(),success:function(n){if(n!=null){var k=n.result;var p=n.title;var m=n.message;var q=n.returnUrl;var j=n.additionMsgYn;var l=false;var o="";if(p!=""&&p!=null){o+=p}if(m!=""&&m!=null){if(p!=""&&p!=null){o+="\n"}o+=m}if(k=="success"){l=true;if(i!=""&&i!=null){if($.trim(j)=="true"){i+="\n";i+=o}o=i}}else{if(k=="fail"){l=false}else{o="Invalid Networking\nEmpty Result";q=g;l=false}}if(o!=""&&o!=null){alert(o)}if(f!=null&&typeof(f)!="undefined"){f(n)}if(q!=""&&q!=null){removeFormObj(a);q=contextPath+q;if(typeof(b)!="undefined"){b(q,l);disableDim()}else{location.href=q}}else{disableDim()}a.attr("action",e);return l}else{removeFormObj(a);alert("Invalid Networking\nNo Data");location.href=g}},error:function(k,j,l){removeFormObj(a);alert("Invalid Networking\nNetwork Error. Please try again later.");location.href=g}})}function enableDim(){$("#loading_dim").show()}function disableDim(){$("#loading_dim").hide()}function removeFormObj(b){var a=b.serializeArray();$.each(a,function(){$("input[name="+this.name+"]").val("")})}function normalCustomerAjax(c,e,a,d,b){enableDim();$.ajax({type:"POST",url:c,cache:false,data:e,dataType:"json",success:function(j){var h=j;if(h!=null&&h!=""){var f=h.result;if(f!=null&&f!=""){if(f=="success"){var g=h.successMsg;a(g)}else{if(f=="fail"){var i=h.errorMsg;d(i);disableDim()}else{b()}}}else{b()}}else{b()}},error:function(h,f,g){b(h,f,g);disableDim()}})}$(document).ready(function(){if($(".pageloading_wrap").length<1){var a='<div id="loading_dim" class="pageloading_wrap payment" style="display:none;">    <div class="loading_dimmed"></div>    <div id="canvasloader-white" class="loading">        <img src="'+commonResourcePath+'/images/ajax_loader_gray_48.gif" alt="" />    </div></div>';$("body").prepend(a)}});