var spinner={_wrap:null,getProtocol:function(){var a="shilladfs";if(ACC.config.contextPath.endWith("/zh")){a+="cn"}else{if(ACC.config.contextPath.endWith("/ja")){a+="jp"}}return a},load:function(b){var a="";if(b=="transparent"){a+='<div class="pageloading_wrap onlyMain">';a+='	<div class="loading" id="canvasloader-black">';a+='		<img src="'+commonResourcePath+'/images/ajax_loader_gray_48.gif">';a+="	</div>";a+="</div>"}else{if(b=="noApp"){if(!isApp){a+='<div class="pageloading_wrap onlyMain">';a+='	<div class="loading" id="canvasloader-black">';a+='		<img src="'+commonResourcePath+'/images/ajax_loader_gray_48.gif">';a+="	</div>";a+="</div>"}}else{a+='<div class="pageloading_wrap">';a+='	<div class="loading_dimmed"></div>';a+='	<div class="loading" id="canvasloader-black">';a+='		<img src="'+commonResourcePath+'/images/ajax_loader_gray_48.gif">';a+="	</div>";a+="</div>"}}$("body").prepend(a);this._wrap=$("div.pageloading_wrap")},show:function(){if(!this._wrap){this._wrap=$("div.pageloading_wrap")}if(this._wrap.length==0){return}this._wrap.show()},hide:function(){if(!this._wrap){this._wrap=$("div.pageloading_wrap")}if(this._wrap.length==0){return}this._wrap.hide()}};