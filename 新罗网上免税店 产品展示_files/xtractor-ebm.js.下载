var ebmCookieName="personalizeAdYN";var serverHost="https://ebm.shilladfs.com:4000";var DEV_serverHost="http://72.2.30.150:8080";var HTTPS_DEV_serverHost="https://ebmdev.shilladfs.com:4000";function EBMRequest(){var a="";this.getParameter=function(f){var c=unescape(location.href);var e=(c.substring(c.indexOf("?")+1,c.length)).split("&");for(var d=0;d<e.length;d++){var b=e[d].split("=");if(b[0].toUpperCase()==f.toUpperCase()){a=e[d].split("=")[1];break}}return a}}function checkEBM(){var f=new EBMRequest();var i="";var a="PC";var h=document.domain;var e=f.getParameter("uiel");if(typeof h!="undefined"){if(h.indexOf("m")==0||"Mobile"==e){a="M"}}i+="device_type="+a+"&";var j=f.getParameter("EBM_TEST");if("Y"==j){var c=f.getParameter("TEST_POP_URL");var d=f.getParameter("TEST_POP_WIDTH");var l=f.getParameter("TEST_POP_HEIGHT");var k=f.getParameter("TEST_POP_TOP");var b=f.getParameter("TEST_POP_LEFT");var g=f.getParameter("TEST_POP_TYPE");i+="force_popup=Y";i+="&TEST_POP_URL="+c;i+="&TEST_POP_WIDTH="+d;i+="&TEST_POP_HEIGHT="+l;i+="&TEST_POP_TOP="+k;i+="&TEST_POP_LEFT="+b;i+="&TEST_POP_TYPE="+g;console.log("test ebm...");callEBM(i+"&")}else{callEBM(i)}}function callEBM(e){var d=getXTCookie("XTVID");var a=getXTCookie("XTLID");var b=location.pathname;var c=document.domain;if(c.indexOf("stage")>-1||c.indexOf("dev")>-1||c.indexOf("2")>-1){if(location.protocol.indexOf("https")>-1){serverHost=HTTPS_DEV_serverHost}else{serverHost=DEV_serverHost}}var f=$.getJSON(serverHost+"/ebm/EbmMkt?"+e+"user_id="+a+"&v_id="+d+"&ebm_type=event_chk&req_path="+b+"&req_type=json&callback=?",function(h,g,i){}).done(function(i){var h=i.popup_id;if(typeof h!="undefined"){var g=i.popup_type;if("2"==g){createLayerEBM(i)}else{if("3"==g){createLayerEBM(i)}else{if("1"==g){createPopupEBM(i)}}}}else{}}).fail(function(h,g,i){console.log("error func");console.log(i)}).always(function(h,g,i){})}function jsonCallback(a){}function createLayerEBM(c){var b=c.width+1;var a=c.heigth+1;var d=decodeURIComponent(c.popup_html.replace(/\+/g," "));$("body").append(d)}function createToastEBM(c){var b=c.width+1;var a=c.heigth+1;var d=decodeURIComponent(c.popup_html.replace(/\+/g," "));$("body").append(d)}function createPopupEBM(g){var f=g.width;var c=g.height;var e=g.top;var d=g.left;var b=g.popup_url;var a="width="+f+", height="+c+", top="+e+", left="+d;window.open(b,"_blank",a)}function getXTCookie(b){var e=document.cookie.split("; ");for(var c=0;c<e.length;c++){var d=e[c].indexOf("=");var a=e[c].substring(0,d);if(a==b){return unescape(e[c].substring(d+1))}}return""}function existEBMCookie(b){var a=getXTCookie(b);if(a!=null&&a!=""){return true}return false}function getPrivateMarketingOffr(a,b){$.ajax({type:"POST",url:contextPath+"/event/eventSimplePartView/eventPartPrivate",dataType:"json",cache:true,crossDomain:true,data:{eventId:a,offrId:b},success:function(c,d,f){var e="";if(c.result=="success"){e="혜택이 지급 되었습니다.";if(typeof Common!=="undefined"){Common.Toast.basic(e);SUI.Utils.fadeOut(".private_popup")}else{alert(e);SUI.Utils.fadeOut(".private_popup")}}else{if(c.message!=="undefined"&&c.message!=null&&c.message!=""){e=c.message}else{e="지급에 실패하였습니다"}if(typeof Common!=="undefined"){Common.Toast.basic(e)}else{alert(e)}}},error:function(e,c,d){if(typeof Common!=="undefined"){Common.Toast.basic("지급에 실패하였습니다")}else{alert("지급에 실패하였습니다")}}})}setTimeout(function(){checkEBM()},500);